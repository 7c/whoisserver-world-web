{
  "name": "@7c/whoisserver-world-web",
  "version": "1.0.31",
  "description": "Web-ready TypeScript library that wraps the whoisserver-world dataset.",
  "repository": {
    "type": "git",
    "url": "https://github.com/7c/whoisserver-world-web"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/whoisserver-world-web.cjs",
  "module": "dist/whoisserver-world-web.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/whoisserver-world-web.mjs",
      "require": "./dist/whoisserver-world-web.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --emitDeclarationOnly && vite build",
    "typecheck": "tsc --noEmit",
    "dev": "vite",
    "clean": "rm -rf dist",
    "snapshot:data": "tsx scripts/snapshot-data.ts"
  },
  "keywords": [
    "typescript",
    "library",
    "vite",
    "json"
  ],
  "devDependencies": {
    "@types/punycode": "^2.1.4",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "punycode": "^2.3.1",
    "whoisserver-world": "^1.0.31"
  }
}
